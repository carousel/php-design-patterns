{
  "name": "carousel/php-design-patterns",
  "description": "PHP design patterns adopted from https://sourcemaking.com",
  "type": "project",
  "homepage": "https://github.com/carousel/php-design-patterns",
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "codedungeon/phpunit-result-printer": "dev-master"
    },
  "license": "MIT",
  "authors": [
    {
      "name": "Miroslav Trninic",
      "email": "miroslav.trninic@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Creational\\": "src/gof/creational",
      "Structural\\": "src/gof/structural",
      "Behavioral\\": "src/gof/behavioral",
      "Helpers\\": "src/helpers"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
    "config": {
        "bin-dir": "bin"
    }
}
